package example.jpa;

import javax.persistence.*;
@Embeddable
public class Department {
	
	private String dept;
	
	
	public int getId() {
		return Id;
	}
	public void setId(int id) {
		Id = id;
	}
	public String getDept() {
		return dept;
	}
	public void setDept(String dept) {
		this.dept = dept;
	}
	public Department() {
		super();
		// TODO Auto-generated constructor stub
	}

	}
	
	

