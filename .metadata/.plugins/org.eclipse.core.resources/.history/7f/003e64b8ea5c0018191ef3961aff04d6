package example.jpa;

import javax.persistence.*;
@Entity
public class Department {
	private int Id;
	private String dept;
	
	
	public int getId() {
		return Id;
	}
	public void setId(int id) {
		Id = id;
	}
	public String getDept() {
		return dept;
	}
	public void setDept(String dept) {
		this.dept = dept;
	}

	}
	
	

